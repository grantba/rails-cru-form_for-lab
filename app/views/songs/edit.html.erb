<%= form_for @song do |f| %>

    <%= f.label "Song Name" %><br>
    <%= f.text_field :name %><br><br>

    <%= f.label "Artist ID" %><br>
    <%= f.number_field :artist_id %><br><br>

    <%= f.label "Genre ID" %><br>
    <%= f.number_field :genre_id %><br><br>

    <%= f.submit "Update Song" %>
    
<% end %>